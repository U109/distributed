<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzz.producer.mapper.AccountInfoMapper">

    <resultMap id="BaseResultMap" type="com.zzz.producer.entity.AccountInfo">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="accountName" column="account_name" jdbcType="VARCHAR"/>
        <result property="accountNo" column="account_no" jdbcType="VARCHAR"/>
        <result property="accountPassword" column="account_password" jdbcType="VARCHAR"/>
        <result property="accountBalance" column="account_balance" jdbcType="DOUBLE"/>
    </resultMap>

    <sql id="Base_Column_List">
        id
        ,account_name,account_no,
        account_password,account_balance
    </sql>
</mapper>
